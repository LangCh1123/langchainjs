{
  "buildCommand": "yarn build",
  "trailingSlash": true,
  "redirects": [
    {
      "source": "/:path*/langchain_:rest*",
      "destination": "/:path*/_langchain_:rest*"
    },
    {
      "source": "/v0.2/:path*/langchain_:rest*/:file*",
      "destination": "/v0.2/:path*/_langchain_:rest*/:file*"
    }
  ]
}
