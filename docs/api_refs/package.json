{
  "name": "api_refs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "typedoc": "npx typedoc --options typedoc.json",
    "build:scripts": "node ./scripts/generate-api-refs.js && node ./scripts/update-typedoc-css.js",
    "prebuild": "yarn turbo build --filter=langchain --filter=@langchain/openai --filter=@langchain/anthropic",
    "build2": "yarn prebuild && yarn build:scripts && next build",
    "build": "yarn turbo run build:next",
    "build:next": "next build",
    "start": "yarn build && next start -p 3001",
    "lint": "next lint"
  },
  "dependencies": {
    "next": "14.0.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-morph": "^20.0.0",
    "turbo": "latest",
    "typescript": "^5"
  }
}
